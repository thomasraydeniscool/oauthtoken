{
  "defaultSeverity": "error",
  "extends": ["tslint-config-standard", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "variable-name": {
      "options": ["allow-leading-underscore", "allow-snake-case"]
